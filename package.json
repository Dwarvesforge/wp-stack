{
  "name": "dwarvesforge-wp-stack",
  "version": "1.0.0",
  "description": "Wordpress stack that gives you better nice classes as wrapper of the wordpress ones. It also provide a wordpress like api that can be used in template development in order to avoid copy-paste when integrating in a backend theme.",
  "homepage": "https://dwarvesforge.io",
  "repository": {
    "type": "git",
    "url": "{project-url}"
  },
  "main": "dist/index.js",
  "watch": {
    "doc": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": "php"
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "concurrently --kill-others \"npm run watch\"",
    "doc": "rm -rf doc/src && npm run doc.php",
    "doc.php": "coffeekraken-docblock-to-markdown -f 'src/**/*.php' -d doc",
    "changelog": "git-changelog --version_name $npm_package_version  --app_name $npm_package_name --intro \"$npm_package_description\" --logo \".resources/coffeekraken-logo.jpg\""
  },
  "keywords": [
    "dwarvesforge",,
    "dwarves",
    "dwarf",
    "wp-stack",
    "wordpress",
    "php",
    "toolkit"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "coffeekraken-carpenter": "^1.0.0",
    "coffeekraken-docblock-to-markdown": "^1.0.0",
    "concurrently": "^3.1.0",
    "git-changelog": "^1.1.2",
    "npm-watch": "0.2.0"
  }
}
